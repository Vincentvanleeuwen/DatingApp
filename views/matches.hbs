{{!--Extends the main layout--}}
{{#> layouts/main }}

  <section class="matches-container">

    {{# each match }}
      {{> matchesChat
        dogName = name
        dogImage = images.[0]
        chatStatus = status
        lastMessage = lastMessage
        email = email
        id = _id
      }}
    {{/ each }}

  </section>

{{/ layouts/main }}